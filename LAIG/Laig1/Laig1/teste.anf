          <descendants>
          </descendants>
        </node>
        <node id="my_unit_cube_Left">
          <transforms>
            <transform type="translate" to="-0.5 0 0" />
            <transform type="rotate" axis="y" angle="-90" />
          </transforms>

          <appearanceref id="inherit" />
          <primitives>
            <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" />
          </primitives>
          <descendants>
          </descendants>
        </node>
        <node id="my_unit_cube_Top">
          <transforms>
            <transform type="translate" to="0 0.5 0" />
            <transform type="rotate" axis="x" angle="-90" />
          </transforms>
          <appearanceref id="inherit" />
          <primitives>
            <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" />
          </primitives>
          <descendants>
          </descendants>
        </node>
        <node id="my_unit_cube_Bottom">
          <transforms>
            <transform type="translate" to="0 -0.5 0" />
            <transform type="rotate" axis="x" angle="90" />
          </transforms>

          <appearanceref id="inherit" />
          <primitives>
            <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" />
          </primitives>
          <descendants>
          </descendants>
        </node>
      </graph>
    </anf>